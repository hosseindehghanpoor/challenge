all:
  hosts:
{% for vm in vm_ips.keys() %}
    {{ vm }}:
      ansible_host: {{ vm_ips[vm] }}
      ansible_user: root
      ansible_ssh_private_key_file: /root/.ssh/SivanLocalServersKey
      ansible_ssh_common_args: '-o StrictHostKeyChecking=no -p 22'
      ansible_port: 22
{% if hostvars[vm].kubekey.master is defined and hostvars[vm].kubekey.master %}
      master: true
{% endif %}
{% endfor %}
